<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object class="tk.Frame" id="top">
    <property name="borderwidth">2</property>
    <property name="height">1500</property>
    <property name="padx">5</property>
    <property name="pady">5</property>
    <property name="width">1500</property>
    <layout>
      <property name="column">0</property>
      <property name="ipadx">75</property>
      <property name="ipady">20</property>
      <property name="padx">5</property>
      <property name="pady">5</property>
      <property name="propagate">True</property>
      <property name="row">0</property>
    </layout>
    <child>
      <object class="tk.Button" id="calibrate">
        <property name="command">on_calibrate</property>
        <property name="text" translatable="yes">Calibrate</property>
        <property name="width">5</property>
        <layout>
          <property name="column">0</property>
          <property name="ipadx">1</property>
          <property name="ipady">1</property>
          <property name="padx">1</property>
          <property name="pady">0</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Button" id="plot">
        <property name="command">on_plot</property>
        <property name="text" translatable="yes">Plot</property>
        <property name="width">5</property>
        <layout>
          <property name="column">1</property>
          <property name="ipadx">1</property>
          <property name="ipady">1</property>
          <property name="padx">1</property>
          <property name="pady">0</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Checkbutton" id="cb_phase">
        <property name="text" translatable="yes">Obtain Phase Data</property>
        <property name="variable">boolean:do_phase</property>
        <property name="width">14</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">3</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Checkbutton" id="cb_linear">
        <property name="offvalue">False</property>
        <property name="onvalue">True</property>
        <property name="text" translatable="yes">Linear Plot</property>
        <property name="variable">boolean:do_linear</property>
        <property name="width">14</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">4</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Label" id="label_min">
        <property name="text" translatable="yes">Min:</property>
        <property name="width">10</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Label" id="label_max">
        <property name="text" translatable="yes">Max:</property>
        <property name="width">10</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">2</property>
        </layout>
      </object>
    </child>
    <!--
    <child>
      <object class="tk.Entry" id="entry_min">
        <property name="text" translatable="yes">0.0</property>
        <property name="textvariable">double:min_val</property>
        <property name="width">10</property>
        <layout>
          <property name="column">1</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
        </layout>
      </object>
    </child>
    
    <child>
      <object class="tk.Entry" id="entry_max">
        <property name="text" translatable="yes">0.0</property>
        <property name="textvariable">double:max_val</property>
        <property name="width">10</property>
        <layout>
          <property name="column">1</property>
          <property name="propagate">True</property>
          <property name="row">2</property>
        </layout>
      </object>
    </child>
    -->
    <child>
      <object class="ttk.Separator" id="separator_horizontal">
        <property name="orient">vertical</property>
        <layout>
          <property name="column">3</property>
          <property name="columnspan">10</property>
          <property name="ipadx">0</property>
          <property name="ipady">300</property>
          <property name="padx">10</property>
          <property name="pady">0</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
          <property name="rowspan">10</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Separator" id="separator_vertical">
        <property name="orient">horizontal</property>
        <layout>
          <property name="column">6</property>
          <property name="columnspan">250</property>
          <property name="ipadx">265</property>
          <property name="padx">10</property>
          <property name="pady">5</property>
          <property name="propagate">True</property>
          <property name="row">4</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
